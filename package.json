{
	"name": "linter-xo",
	"version": "0.30.1",
	"description": "Linter for XO",
	"license": "MIT",
	"repository": "xojs/atom-linter-xo",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "sindresorhus.com"
	},
	"private": true,
	"engines": {
		"atom": ">=1.47.0"
	},
	"scripts": {
		"lint": "xo --ignore='mocks/**/*' --ignore='_test/**/*'",
		"pretest": "npm run lint",
		"test": "ava"
	},
	"files": [
		"lib",
		"mocks",
		"script",
		"spec",
		"_test",
		"*.js"
	],
	"keywords": [
		"xo",
		"xoxo",
		"happy",
		"happiness",
		"lint",
		"linter",
		"eslint",
		"jshint",
		"jslint",
		"javascript",
		"typescript",
		"code",
		"code-style",
		"style",
		"quality",
		"validate",
		"code style",
		"standard",
		"strict",
		"check",
		"checker"
	],
	"dependencies": {
		"atom-linter": "~10.0.0",
		"atom-package-deps": "~5.1.0",
		"eslint-rule-documentation": "~1.0.0",
		"load-json-file": "~6.2.0",
		"p-props": "~4.0.0",
		"pkg-dir": "~4.2.0",
		"resolve-from": "~5.0.0",
		"require-rebuild": "~1.2.0",
		"xo": "~0.40.2"
	},
	"devDependencies": {
		"@babel/register": "^7.9.0",
		"ava": "^3.8.0",
		"electron-rebuild": "^1.11.0",
		"proxyquire": "^2.1.0",
		"text-buffer": "^13.17.0",
		"tmp": "^0.2.0"
	},
	"package-deps": [
		"linter"
	],
	"providedServices": {
		"linter": {
			"versions": {
				"2.2.0": "provideLinter"
			}
		}
	},
	"ava": {
		"require": [
			"@babel/register"
		]
	},
	"babel": {
		"presets": [
			"@ava/stage-4"
		]
	},
	"xo": {
		"globals": [
			"atom"
		]
	}
}
